<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <context-param>
        <description>super admin account</description>
        <param-name>username</param-name>
        <param-value>sonnt5</param-value>
    </context-param>
    <context-param>
        <description>super admin password</description>
        <param-name>password</param-name>
        <param-value>123</param-value>
    </context-param>
    <servlet>
        <servlet-name>StudentInsertController</servlet-name>
        <servlet-class>controller.StudentInsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.accesscontrol.LoginController</servlet-class>
        <init-param>
            <description>url to redirect after successfully logged in.</description>
            <param-name>url</param-name>
            <param-value>dantri.com</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>RectGenerateController</servlet-name>
        <servlet-class>controller.RectGenerateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RectGenerateView</servlet-name>
        <servlet-class>view.RectGenerateView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AController</servlet-name>
        <servlet-class>controller.test.AController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BServlet</servlet-name>
        <servlet-class>controller.test.BServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeCreateController</servlet-name>
        <servlet-class>controller.employee.EmployeeCreateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeUpdateController</servlet-name>
        <servlet-class>controller.employee.EmployeeUpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeListController</servlet-name>
        <servlet-class>controller.employee.EmployeeListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controller.home.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeDeleteController</servlet-name>
        <servlet-class>controller.employee.EmployeeDeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmloyeeSearchController</servlet-name>
        <servlet-class>controller.employee.EmloyeeSearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeDetailController</servlet-name>
        <servlet-class>controller.employee.EmployeeDetailController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.accesscontrol.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmployeeFilterController</servlet-name>
        <servlet-class>controller.employee.EmployeeFilterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <servlet-class>controller.productionplan.ProductionPlanCreateController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentInsertController</servlet-name>
        <url-pattern>/student/insert</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>StudentLoadController</servlet-name>
        <servlet-class>controller.StudentLoadController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentLoadController</servlet-name>
        <url-pattern>/student/load</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RectGenerateController</servlet-name>
        <url-pattern>/rect/gen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RectGenerateView</servlet-name>
        <url-pattern>/view/rect/gen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AController</servlet-name>
        <url-pattern>/a</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BServlet</servlet-name>
        <url-pattern>/b</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeCreateController</servlet-name>
        <url-pattern>/employee/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeUpdateController</servlet-name>
        <url-pattern>/employee/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeListController</servlet-name>
        <url-pattern>/employee/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeDeleteController</servlet-name>
        <url-pattern>/employee/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmloyeeSearchController</servlet-name>
        <url-pattern>/employee/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeDetailController</servlet-name>
        <url-pattern>/employee/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeFilterController</servlet-name>
        <url-pattern>/employee/filter</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ProductionPlanCreateController</servlet-name>
        <url-pattern>/productionplan/create</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>25</session-timeout>
    </session-config>
</web-app>
